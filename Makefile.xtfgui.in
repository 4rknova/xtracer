PATH_SRC_FRONTEND_GUI = $(PATH_SRC)/xt_gyi

FRONTEND_GUI_SRC_H   = $(wildcard $(PATH_SRC_FRONTEND_GUI)/*.h)
FRONTEND_GUI_SRC_HPP = $(wildcard $(PATH_SRC_FRONTEND_GUI)/*.hpp)
FRONTEND_GUI_SRC_INL = $(wildcard $(PATH_SRC_FRONTEND_GUI)/*.inl)
FRONTEND_GUI_SRC_TML = $(wildcard $(PATH_SRC_FRONTEND_GUI)/*.tml)
FRONTEND_GUI_SRC_C   = $(wildcard $(PATH_SRC_FRONTEND_GUI)/*.c)
FRONTEND_GUI_SRC_CC  = $(wildcard $(PATH_SRC_FRONTEND_GUI)/*.cc)
FRONTEND_GUI_SRC_CPP = $(wildcard $(PATH_SRC_FRONTEND_GUI)/*.cpp)
FRONTEND_GUI_SRC_C11 = $(wildcard $(PATH_SRC_FRONTEND_GUI)/*.c11)

FRONTEND_GUI_BINOBJ_C   = $(addprefix $(PATH_OBJ)/, $(patsubst $(PATH_SRC_FRONTEND_GUI)/%,%,$(FRONTEND_GUI_SRC_C:.c=.o)))
FRONTEND_GUI_BINOBJ_CC  = $(addprefix $(PATH_OBJ)/, $(patsubst $(PATH_SRC_FRONTEND_GUI)/%,%,$(FRONTEND_GUI_SRC_CC:.cc=.o)))
FRONTEND_GUI_BINOBJ_CPP = $(addprefix $(PATH_OBJ)/, $(patsubst $(PATH_SRC_FRONTEND_GUI)/%,%,$(FRONTEND_GUI_SRC_CPP:.cpp=.o)))
FRONTEND_GUI_BINOBJ_C11 = $(addprefix $(PATH_OBJ)/, $(patsubst $(PATH_SRC_FRONTEND_GUI)/%,%,$(FRONTEND_GUI_SRC_C11:.c11=.o)))

FRONTEND_GUI_BINOBJ = $(sort $(FRONTEND_GUI_BINOBJ_C) $(FRONTEND_GUI_BINOBJ_CC) $(FRONTEND_GUI_BINOBJ_CPP) $(FRONTEND_GUI_BINOBJ_C11))

FRONTEND_GUI_BINPATHS = $(sort $(dir $(FRONTEND_GUI_BINOBJ)))

XTCORE_FRONTEND_GUI = $(PATH_BIN)/$(SW_PACKAGE)_gui
