PATH_SRC_FRONTEND_CLI = $(PATH_SRC)/xt_cli

FRONTEND_CLI_SRC_H   = $(wildcard $(PATH_SRC_FRONTEND_CLI)/*.h)
FRONTEND_CLI_SRC_HPP = $(wildcard $(PATH_SRC_FRONTEND_CLI)/*.hpp)
FRONTEND_CLI_SRC_INL = $(wildcard $(PATH_SRC_FRONTEND_CLI)/*.inl)
FRONTEND_CLI_SRC_TML = $(wildcard $(PATH_SRC_FRONTEND_CLI)/*.tml)
FRONTEND_CLI_SRC_C   = $(wildcard $(PATH_SRC_FRONTEND_CLI)/*.c)
FRONTEND_CLI_SRC_CC  = $(wildcard $(PATH_SRC_FRONTEND_CLI)/*.cc)
FRONTEND_CLI_SRC_CPP = $(wildcard $(PATH_SRC_FRONTEND_CLI)/*.cpp)
FRONTEND_CLI_SRC_C11 = $(wildcard $(PATH_SRC_FRONTEND_CLI)/*.c11)

FRONTEND_CLI_BINOBJ_C   = $(addprefix $(PATH_OBJ)/, $(patsubst $(PATH_SRC_FRONTEND_CLI)/%,%,$(FRONTEND_CLI_SRC_C:.c=.o)))
FRONTEND_CLI_BINOBJ_CC  = $(addprefix $(PATH_OBJ)/, $(patsubst $(PATH_SRC_FRONTEND_CLI)/%,%,$(FRONTEND_CLI_SRC_CC:.cc=.o)))
FRONTEND_CLI_BINOBJ_CPP = $(addprefix $(PATH_OBJ)/, $(patsubst $(PATH_SRC_FRONTEND_CLI)/%,%,$(FRONTEND_CLI_SRC_CPP:.cpp=.o)))
FRONTEND_CLI_BINOBJ_C11 = $(addprefix $(PATH_OBJ)/, $(patsubst $(PATH_SRC_FRONTEND_CLI)/%,%,$(FRONTEND_CLI_SRC_C11:.c11=.o)))

FRONTEND_CLI_BINOBJ = $(sort $(FRONTEND_CLI_BINOBJ_C) $(FRONTEND_CLI_BINOBJ_CC) $(FRONTEND_CLI_BINOBJ_CPP) $(FRONTEND_CLI_BINOBJ_C11))

FRONTEND_CLI_BINPATHS = $(sort $(dir $(FRONTEND_CLI_BINOBJ)))

XTCORE_FRONTEND_CLI = $(PATH_BIN)/$(SW_PACKAGE)_cli
